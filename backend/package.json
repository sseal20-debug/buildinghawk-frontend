{
  "name": "industrial-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Industrial Property Tracker",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "db:init": "node src/db/init.js",
    "db:seed": "node src/db/seed-sample.js",
    "db:import": "node src/db/import-all.js",
    "geocode": "node src/scripts/geocode.js",
    "geocode:google": "node src/scripts/geocode.js --google",
    "import:data": "node src/scripts/import-db.js",
    "create:geojson": "node src/scripts/create-geojson.js",
    "export:data": "node src/scripts/export-from-supabase.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "shapefile": "^0.6.6",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  }
}
